{
    "type": "Group",
    "name": "Group main",
    "style": {
        "margin": "60px"
    },
    "contents": [
        {
            "type": "HtmlText",
            "name": "Widget",
            "text": "<h2>HTML Text In 4th group0</h2>",
            "style": {
                "margin": "60px"
            },
            "event": {
                "click": "alert('Wow!! Click event');",
                "mouseover": "$widget.style({\"background\": {\"color\": \"red\"}});",
                "mouseout": "$widget.style({\"background\": {\"color\": null}});",
                "resize": "if($media(\"gt-sm\")){$widget.style(\"background.color\", \"red\");}else{$widget.style(\"background.color\", \"blue\");}"
            }
        }
    ]
}